<script>
	// 随机 变化的淡入
var bgarr = ['<%= theme.textarrement.S.color.pc1 %>','<%= theme.textarrement.S.color.pc2 %>','<%= theme.textarrement.S.color.pc3 %>','<%= theme.textarrement.S.color.pc4 %>','<%= theme.textarrement.S.color.pc5 %>','<%= theme.textarrement.S.color.pc6 %>','<%= theme.textarrement.S.color.pc7 %>','<%= theme.textarrement.S.color.pc8 %>','<%= theme.textarrement.S.color.pc9 %>','<%= theme.textarrement.S.color.pc10 %>','<%= theme.textarrement.S.color.pc11 %>']; 

var index = Math.floor((Math.random()*bgarr.length)); 
		
$(".dearmsdan-post").mouseover(function(){		  
	// 预期不佳
	$('.dearmsdan-inner-content:hover').css("background-color",bgarr[index]);
	
});
$(".dearmsdan-post").mouseleave(function(){
	// 移除
	$('.dearmsdan-inner-content').css("background-color",'transparent');
		
});

</script>
	
