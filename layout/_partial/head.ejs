<%# head头部信息 %>
  <%
	
  var title = page.title;

  if (is_archive()){
    title = __('archive');

    if (is_month()){
      title += '：' + page.year + '/' + page.month;
    } else if (is_year()){
      title += '：' + page.year;
    }
  } else if (is_category()){
    title = __('count.categories') + '：' + page.category;
  } else if (is_tag()){
    title = __('count.tags') + '：' + page.tag;
  }
  %>

<head>
	<title><% if (title){ %><%= title %> - <% } %> <%= config.title %></title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta name="description" content="<%= config.description %>">
	<meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
	<link rel="shortcut icon" href="<%= theme.favicon.href %>" type="<%= theme.favicon.type %>" />
	
	<%# 阿里图标库 %>
	<link rel="stylesheet" type="text/css" href="<%= theme.aliiconfont %>">
	<%# 引入配置css文件 %>
	<link rel="stylesheet" type="text/css" href="<%= theme.bootstrap.css %>">
	<%- css('css/style.css') %>
	<%# 乔巴拖尾 %>
	<link rel="stylesheet" type="text/css" href="<%= theme.title_qiaoba %>">
	<%# 社交按钮css %>
	<link rel="stylesheet" type="text/css" href="<%= theme.social.css %>">
	
	<%# 文章统计样式dearmsdan-post-Statistics %>
	<link rel="stylesheet" type="text/css" href="<%= theme.Statistics %>">
	
	
	<%# 引入配置js 文件 %>
	<script src="<%= theme.jquery.js.one %>"></script>
	<%- js('js/app.js') %>
	<script src="<%= theme.bootstrap.js %>"></script>
	<%# 引入滚动条js 文件 %>
	<script src="<%= theme.slimscroll %>"></script>
	<%# 引入全文搜索js 文件 %>
	<script src="<%= theme.searchbtn %>"></script>
	
	<%# 网站加载条 %>
	<script src="<%= theme.loadingBar.pace %>"></script>
	<link rel="stylesheet" href="<%= theme.loadingBar.pink %>">
	
	<%# PJAX %>
	<script src="<%= theme.jquerypjax.pjax %>"></script>
	<%# PJAX js %>
	<script src="<%= theme.jquerypjax.index %>"></script>
	<%# 引入评论valine.js %>
	<script src="<%= theme.valine %>"></script>
	
	<%# 友链 %>
	<link rel="stylesheet" href="<%= theme.link.css %>">
	
	<%# 代码高亮 %>
	<link rel="stylesheet" href="<%= theme.prettyPrint.css %>">
	<script src="<%= theme.prettyPrint.js %>"></script>
	
	<%# 可能是文章自动生成目录及侧边栏目录滚动特效的插件toc-helper %>
	<link href="<%= theme.tochelper.css %>" rel="stylesheet" />
	<script src="<%= theme.tochelper.js %>"></script>	
	
	<%# 图片放大preview %>
	<link href="<%= theme.preview.css %>" rel="stylesheet" />
	<script src="<%= theme.preview.js %>"></script>
	<script src="<%= theme.preview.min %>"></script>
	
</head>



